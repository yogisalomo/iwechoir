<h1>New Member</h1>
<%= form_for :member, url: admin_member_path(@member), method: :patch do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :birthday %><br>
    <%= f.text_field :birthday %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :phone %><br>
    <%= f.text_field :phone %>
  </p>

  <p>
    <%= f.label :role %><br>
    <%= f.text_field :role %>
  </p>

  <p>
    <%= f.label :active_start %><br>
    <%= f.text_field :active_start %>
  </p>

  <p>
    <%= f.label :active_end %><br>
    <%= f.text_field :active_end %>
  </p>

  <p>
    <%= f.label :image %><br>
    <%= f.text_field :image %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>