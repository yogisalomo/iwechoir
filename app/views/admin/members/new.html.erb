<% content_for :content do %>
<div class="row">
  <h1>New Member</h1>  
</div>
<div class="row">
  <div class="col-lg-6">
    <%= form_for :member, url: admin_members_path, html: {role: "form"} do |f| %>
        <div class="form-group">
            <%= f.label :name %>
            <%= f.text_field :name, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :email %>
            <%= f.text_field :email, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :phone %>
            <%= f.text_field :phone, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :birthday %>
            <%= f.text_field :birthday,class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :role %>
            <%= f.text_field :role, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :active_start %>
            <%= f.text_field :active_start, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :active_end %>
            <%= f.text_field :active_end, class: 'form-control' %>
            <p class="help-block">Example block-level help text here.</p>
        </div>
        <div class="form-group">
            <%= f.label :image %>
            <%= f.file_field :image %>
        </div>
        <button type="submit" class="btn btn-default">Submit Button</button>
    <% end %>
  </div> 
</div>

<% end %>